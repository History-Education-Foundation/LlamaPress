<div id="<%= dom_id static_web_page_history %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= static_web_page_history.content %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Static web page:</strong>
    <%= static_web_page_history.static_web_page_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Prompt:</strong>
    <%= static_web_page_history.prompt %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User message:</strong>
    <%= static_web_page_history.user_message %>
  </p>

</div>
